    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "prisma": "^3.4.2",
    "typescript": "^4.4.4"
  },
  "type": "module",
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "*.{ts,js}": "eslint --cache --quiet --fix"

    "cross-fetch": "^3.1.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^5.5.1",
    "graphql": "^15.6.1",
    "graphql-scalars": "^1.12.0",
    "graphql-tools": "^8.2.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "pino": "^7.0.5",
    "pino-pretty": "^7.1.0"
